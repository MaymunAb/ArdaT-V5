InfoArdaT.Image = "http://www.roblox.com/asset/?id=6026568210"
InfoArdaT.MouseButton1Down:Connect(function()
	if not Debounce then
		Info.Visible = true
		Debounce = true
	else
		Info.Visible = false
		Debounce = false
	end
end)

